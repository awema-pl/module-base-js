{
    "name": "awema-pl/module-base-js",
    "description": "Base for modules Awema.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Karol Golec",
            "email": "karolgolec10@gmail.com",
            "homepage": "https://github.pl/awema-pl/module-base-js",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "karolgolec10@gmail.com"
    },
    "homepage": "https://github.com/awemapl/basejs",
    "keywords": [
        "Laravel",
        "BaseJS"
    ],
    "require": {
        "composer/composer": "^1.8",
        "illuminate/support": "~5|~6|~7|~8|~9"
    },
    "autoload": {
        "psr-4": {
            "AwemaPL\\BaseJS\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AwemaPL\\BaseJS\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --colors=always"
    },
    "extra": {
        "laravel": {
            "providers": [
                "AwemaPL\\BaseJS\\BaseJSServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
